<% content_for(:title, t('.title')) %>
<div class="min-h-screen container mx-auto">
  <div class="overflow-x-auto">
    <%= render 'matchings/search', q: @q, url: (action_name == 'index' ? matchings_path : send("#{action_name}_path")) %>
      <div class="divider"><p class='text-2xl font-bold'><%= t('.title') %></p></div>
    <%= render 'matchings/matchings', matchings: @matchings %>
  </div>
  <%= paginate @matchings %>
</div>
